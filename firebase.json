{
    "hosting": {
      "public": "dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "function": "ssrapp",
          "destination": "/404.html"
        }
      ],
      "headers": [ {
        "source": "**/*.@(jpg|jpeg|gif|png)",
        "headers": [
            {
              "key": "Cache-Control",
              "value": "max-age=604800"
            }
          ]
        }
      ]
    },
    "emulators": {
      "ui": {
        "enabled": true
      }
    }
  }
