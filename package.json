{
  "name": "ccv-website-experimental",
  "version": "1.0.0",
  "private": true,
  "description": "CCV Website Experimental in Nuxt.js",
  "author": "fernandogelin",
  "scripts": {
    "build": "nuxt build",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "commit": "git-cz",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "start": "nuxt start",
    "test": "jest"
  },
  "dependencies": {
    "@brown-ccv/disco-styles": "^2.2.1",
    "@brown-ccv/disco-vue-components": "^1.2.2",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "bulma": "^0.9.0",
    "frontmatter-markdown-loader": "^3.4.3",
    "nuxt": "^2.14.6",
    "v-runtime-template": "^1.10.0"
  },
  "devDependencies": {
    "@nuxt/components": "^1.1.0",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "git-cz": "^4.3.1",
    "husky": "^4.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.0",
    "node-sass": "^4.14.1",
    "nuxt-svg-loader": "^1.2.0",
    "prettier": "^1.19.1",
    "sass-loader": "^8.0.2",
    "stylelint": "^10.1.0",
    "vue-cli-plugin-cz": "^1.2.2",
    "vue-jest": "^4.0.0-0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  }
}
